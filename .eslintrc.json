{
  "env": {
    "commonjs": true,
    "es2021": true
  },
  "extends": [
    "semistandard",
    "standard-jsx",
    "standard-react"
  ],
  "rules": {
    "comma-dangle": ["error", {
      "arrays": "always-multiline",
      "objects": "always-multiline",
      "imports": "always-multiline",
      "exports": "always-multiline",
      "functions": "never"
    }]
  }
}
